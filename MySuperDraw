draw line(instant, filled, green, 100, 100, 200, 150, 10)
 
draw square(instant, empty, blue, 400, 100, 60)
 
draw rectangle(instant, filled, yellow, 150, 300, 60, 30)
 
draw triangle(instant, empty, red, 450, 300, 60)
 
draw circle(animated, filled, green, 250, 200, 60)
 
draw arc(instant, filled, yellow, 300, 400, 60, 30, 10)
 
draw ellipse(instant, filled, yellow, 500, 400, 60, 30)
 
var i = 3
while (i < 12) {
draw polygon(instant, filled, blue, 550, 400 + 10, 60, i)
i = i+1
}